    def $action_name$(self$add_method_params$, $body_type_lc$=params.$body_type$()$headers_method_params_str$):
$docs$
        url = '/$url$'

        result = self.__getProxy().request(
            method='$method$',
            url=UrlHelper.replace(
                url,
                {'{$parent_resource_name_lc$:id}' : self.parentclass.get_id()}
